cabal-version: 2.4
name:          spinor
version:       0.1.0.0
build-type:    Simple

executable spinor
  main-is:          Main.hs
  other-modules:    Syntax
  build-depends:
    base       >= 4.14 && < 5,
    megaparsec >= 9.0  && < 10,
    text       >= 1.2  && < 3
  default-language: Haskell2010
  ghc-options:      -Wall
